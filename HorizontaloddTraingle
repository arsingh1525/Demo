import java.util.Scanner;

public class horizontaloddnumber {


        public static void main(String args[])
        {
            Scanner Sc=new Scanner(System.in);
            System.out.println("enter row ");
            int n=Sc.nextInt();

            for(int i=1; i<=n; i++)
            {

                for(int j=1; j<=i; j++)
                {
                    System.out.print(2*j-1 + " ");
                }

                System.out.println();
            }
        }
    }
